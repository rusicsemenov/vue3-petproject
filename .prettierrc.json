{
    "$schema": "https://json.schemastore.org/prettierrc",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "vueIndentScriptAndStyle": true,
    "htmlWhitespaceSensitivity": "ignore",
    "endOfLine": "auto",
    "printWidth": 100,
    "plugins": ["prettier-plugin-tailwindcss"]
}
